        <servlet-class>controller.mechanic.searchTicketMechanicServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>serviceTicketMechanicDetailServlet</servlet-name>
        <servlet-class>controller.mechanic.serviceTicketMechanicDetailServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateServiceTicketMechanicServlet</servlet-name>
        <servlet-class>controller.mechanic.updateServiceTicketMechanicServlet</servlet-class>
