    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginCusServlet</servlet-name>
        <url-pattern>/loginCusServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateProfileSerlvet</servlet-name>
        <url-pattern>/updateProfileSerlvet</url-pattern>
    </servlet-mapping>
