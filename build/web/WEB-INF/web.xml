        <servlet-name>loginMechanicServlet</servlet-name>
        <url-pattern>/loginMechanicServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MainMechanic</servlet-name>
        <url-pattern>/MainMechanic</url-pattern>
    </servlet-mapping>
